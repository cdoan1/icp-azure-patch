apiVersion: v1
kind: Config
clusters:
- cluster:
    insecure-skip-tls-verify: true
    server: {{ kubernetes_apiserver_url }}
  name: cfc-cluster
contexts:
- context:
    cluster: cfc-cluster
    user: cloud-controller-manager
users:
- name: cloud-controller-manager
  user:
    client-certificate: /etc/cfc/conf/cloud-controller-manager.crt
    client-key: /etc/cfc/conf/cloud-controller-manager.key
